{
  "name": "yonadav_calendar",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "build": "(if not exist dist mkdir dist) && browserify js/main.js -t babelify -o dist/bundle.js && copyfiles index.html \"styles/*\" dist",
    "watch:js": "browserify js/main.js -t babelify -o dist/bundle.js -w",
    "serve": "live-server dist",
    "dev": "npm run build && npm-run-all --parallel watch:js serve",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YairMSIl/yonadav_calendar.git"
  },
  "keywords": [],
