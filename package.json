  "bugs": {
    "url": "https://github.com/YairMSIl/yonadav_calendar/issues"
  },
  "homepage": "https://github.com/YairMSIl/yonadav_calendar#readme",
  "description": "",
  "dependencies": {
    "@hebcal/core": "^5.10.1",
    "@hebcal/locales": "^6.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.1",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5"
  }
}
